;	   Revision History
;
; Rev:   Date:    Reason:
; 1.02  03/25/99  Added new devices
; 1.01  12/16/96  Added new devices
; 1.00  07/18/96  Initial release
;==========================================================================
;
;       Verify Processor
;
;==========================================================================

        .IFDEF __EEPROM8
.define __IS_MEMORY
        .ENDIF
       
        .IFDEF __EEPROM16
.define __IS_MEMORY
        .ENDIF
       
        .IFNDEF __IS_MEMORY
           .MSG "Processor-header file mismatch.  Verify selected processor."
        .ELSE
.undefine __IS_MEMORY
        .ENDIF

