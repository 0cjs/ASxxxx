;Configuration Byte 0 Options
_CP_ON_0          =:  0x00	; Code Protect enable   
_CP_OFF_0         =:  0xFF

;Configuration Byte 1 Options
_OSCS_ON_1        =:  0xDF	; Oscillator Switch enable
